pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo this is Build stage'
            }
        }
        stage('Test') {
            steps {
                sh 'echo this is Test stage'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo this is deployment stage'
            }
        }
    }
}